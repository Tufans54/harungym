<script>
    // --- PROGRAM TABLARI Ä°Ã‡Ä°N JS ---
    function openDay(evt, dayName) {
        var i, tabcontent, tablinks;
        
        tabcontent = document.getElementsByClassName("day-content");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].classList.remove('active');
        }

        tablinks = document.getElementsByClassName("tab-button");
        for (i = 0; i < tablinks.length; i++) {
            tablinks[i].classList.remove('active');
        }

        document.getElementById(dayName).classList.add('active');
        evt.currentTarget.classList.add('active');
    }

    // --- ANA FÄ°LTRE VE GÃ–RÃœNÃœRLÃœK JS ---
    document.addEventListener('DOMContentLoaded', function() {
        const filterButtons = document.querySelectorAll('.filter-btn');
        const machineCards = document.querySelectorAll('.gym-card');
        const programWrapper = document.getElementById('program-wrapper');
        const machineGuideWrapper = document.getElementById('machine-guide-wrapper');

        // VarsayÄ±lan: ProgramÄ± gÃ¶ster, kartlarÄ± gizle
        programWrapper.style.display = 'block';
        machineGuideWrapper.style.display = 'none';

        // Pazartesi'yi program aÃ§Ä±ldÄ±ÄŸÄ±nda aktif et (Ä°Ã§ tab sistemi)
        const pazartesiButton = document.querySelector('.tab-buttons .tab-button');
        if (pazartesiButton) {
            const pazartesiContent = document.getElementById('Pazartesi');
            if(pazartesiContent) pazartesiContent.classList.add('active');
            pazartesiButton.classList.add('active');
        }


        filterButtons.forEach(button => {
            button.addEventListener('click', () => {
                filterButtons.forEach(btn => btn.classList.remove('active'));
                button.classList.add('active');

                const filter = button.getAttribute('data-filter');

                if (filter === 'program') {
                    // PROGRAM SEKMESÄ° AÃ‡ILDI
                    machineGuideWrapper.style.display = 'none';
                    programWrapper.style.display = 'block';
                    
                    // Program tablarÄ±nÄ± resetle (Pazartesi'yi gÃ¶ster)
                    const firstProgButton = document.querySelector('#program-wrapper .tab-buttons .tab-button:first-child');
                    if (firstProgButton) {
                        firstProgButton.click(); // Ä°Ã§ tab JS'ini tetikle
                    }
                    return;
                }

                // MAKÄ°NE SEKMELERÄ° AÃ‡ILDI (PROGRAM GÄ°ZLENÄ°R)
                programWrapper.style.display = 'none';
                machineGuideWrapper.style.display = 'block';

                machineCards.forEach(card => {
                    const category = card.getAttribute('data-category');
                    
                    if (filter === 'all' || category === filter) {
                        card.style.display = 'flex';
                    } else {
                        card.style.display = 'none';
                    }
                });
            });
        });
        
        // BaÅŸlangÄ±Ã§ta PROGRAM butonunu aktif et
        document.querySelector('.filter-btn[data-filter="program"]').click();
    });
</script>

